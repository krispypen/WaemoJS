	overlay_stretch = FALSE
      }
    
    # down
    image
      {
        function        = STEPPER
	state           = ACTIVE
	arrow_direction = DOWN
	overlay_file    = "../images/qgn_plat_scroll_thumbsize_vertical_down_arrow_pressed.png"
