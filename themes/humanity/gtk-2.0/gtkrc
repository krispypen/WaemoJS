##############################
#
# Logical color definitions
#
##############################

style "osso-logical-colors" {
      
# text colors
	color["DefaultTextColor"]        = "#ffffff"
	color["SecondaryTextColor"]      = "#999999"
	color["ActiveTextColor"]     	 = "#f47500"
	color["DisabledTextColor"]       = "#666666"
	color["PaintedTextColor"] 	 = "#241100"

# special cases when we need dark text on light background..
	color["ReversedTextColor"]          = "#000000"
	color["ReversedSecondaryTextColor"] = "#808080"
	color["ReversedActiveTextColor"]    = "#f47500"
	color["ReversedDisabledTextColor"]  = "#999999"
	color["ReversedPaintedTextColor"]   = "#ffffff"

# background colors
	color["DefaultBackgroundColor"]   = "#101010"
	color["DarkerBackgroundColor"]    = "#000000"
	color["ReversedBackgroundColor"]  = "#ffffff"
	color["SelectionColor"]           = "#f47500"
	color["ReversedSelectionColor"]	  = "#f47500"

# media /thumbnail /album cover views etc..
	color["ContentBackgroundColor"]	 = "#000000"
	color["ContentFrameColor"]	 = "#000000"
        color["ContentSelectionColor"]	 = "#ffffff"

# framework
	color["TitleTextColor"]		  = "#ffffff"

        color["ButtonTextColor"]	  = "#ffffff"
        color["ButtonTextPressedColor"]	  = "#ffffff"
        color["ButtonTextDisabledColor"]  = "#666666"
        
        color["AlphaKeyColor"]            = "#ff00ff"

# crayons to play with
	color["AccentColor1"]       = "#ff9a3d"
	color["AccentColor2"]       = "#bf5b00"
	color["AccentColor3"]       = "#bf5b00"
	color["AttentionColor"]     = "#e3002a"

# Notification Colors
	color["NotificationBackgroundColor"] = "#efcb00"
	color["NotificationTextColor"] = "#491b03"
	color["NotificationSecondaryTextColor"] = "#783800"

# evil hack
       color["ComboboxBackgroundColor"]	= "#1f1f1f"
}

